{
  "name": "concert-travel-backend",
  "version": "1.0.0",
  "description": "Backend API for Concert Travel App",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "db:setup": "powershell -ExecutionPolicy Bypass -File scripts/setup-db.ps1",
    "db:migrate": "node database/migrate.js run",
    "db:migrate:status": "node database/migrate.js status",
    "db:seed": "psql $DATABASE_URL -f database/init/02-seed.sql",
    "db:reset": "npm run db:setup",
    "db:shell": "psql $DATABASE_URL",
    "setup:artist-metadata": "node setup-artist-metadata.js",
    "test:artist-metadata": "node scripts/test-artist-metadata-simple.js"
  },
  "dependencies": {
    "amadeus": "^11.0.0",
    "aws-sdk": "^2.1490.0",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.0.0",
    "cheerio": "^1.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.0.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.0",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "qrcode": "^1.5.4",
    "rate-limit-redis": "^4.2.1",
    "redis": "^4.6.10",
    "skyscanner-node": "^0.0.2",
    "speakeasy": "^2.0.0",
    "spotify-web-api-node": "^5.0.2",
    "stripe": "^14.10.0",
    "twilio": "^4.19.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
